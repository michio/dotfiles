;;;; git-dwim
;;;====================================
(require 'git-dwim)
(global-set-key "\C-xGb" 'git-branch-next-action)
(global-set-key "\C-xGc" 'git-create-new-branch)
(global-set-key "\C-xGs" 'git-switch-to-other-branch)
(global-set-key "\C-xGm" 'git-merge-to)
